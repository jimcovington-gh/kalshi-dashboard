# Environment variables for Kalshi Dashboard
# Copy this to .env.local and fill in your values

# Cognito Configuration
# Get these from: aws cloudformation describe-stacks --stack-name kalshi-dashboard-cognito
NEXT_PUBLIC_USER_POOL_ID=us-east-1_XXXXXXXXX
NEXT_PUBLIC_USER_POOL_CLIENT_ID=XXXXXXXXXXXXXXXXXXXXXXXXXX
NEXT_PUBLIC_IDENTITY_POOL_ID=us-east-1:XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX

# API Configuration
# Get this from: aws cloudformation describe-stacks --stack-name kalshi-dashboard-api
NEXT_PUBLIC_API_ENDPOINT=https://XXXXXXXXXX.execute-api.us-east-1.amazonaws.com/prod

# AWS Region
NEXT_PUBLIC_AWS_REGION=us-east-1
